<tr>
  <td><%= @time_off_request.user.name %></td>
  <td><%= @time_off_request.start_date.strftime('%D') %></td>
  <td><%= @time_off_request.end_date.strftime('%D') %></td>
  <td><%= status_label(@time_off_request.status) %></td>
  <td><%= time_off_request.created_at.strftime('%D')%></td>
  <td><%= link_to verb(@time_off_request.user_id, current_user.id), edit_time_off_request_path(@time_off_request) ,"data-remote" => true , "data-toggle" => "modal", "data-target" => "#myModal" %></td>
  <td><%= link_to 'Destroy', @time_off_request, method: :delete, data: { confirm: 'Are you sure?' } ,"data-remote" => true , "data-toggle" => "modal", "data-target" => "#myModal" %></td>
</tr>
